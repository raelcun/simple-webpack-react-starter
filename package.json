{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "Bare bones webpack project with a node express server",
  "main": "server/index.js",
  "scripts": {
    "dev-server": "nodemon server/index.js --watch server",
    "dev-web": "webpack-dev-server --config ./webpack-dev.config.js --hot --progress --colors --content-base build",
    "dev": "nodemon server/index.js --watch server & webpack-dev-server --config ./webpack-dev.config.js --hot --progress --colors --content-base build",
    "build": "rm ./build/*; webpack --config ./webpack-prod.config.js --progress --colors",
    "start": "node server/index.js",
    "test": "karma start",
    "testing": "karma start --single-run=false"
  },
  "repository": "git@github.com:raelcun/simple-webpack-react-starter.git",
  "author": "Dan Taylor",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.11.0",
    "cors": "^2.7.1",
    "express": "^4.12.4",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-favicon": "0.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "expect": "^1.19.0",
    "html-webpack-plugin": "^2.16.1",
    "image-webpack-loader": "^1.6.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "nodemon": "^1.3.7",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
